{
  "name": "dbi-backend",
  "version": "0.3.85",
  "description": "Database importer backend",
  "main": "app.ts",
  "scripts": {
    "test": "jest --no-cache --detectOpenHandles --covarage=true",
    "dev": "nodemon -r dotenv/config ./src/index.ts",
    "comp": "tsc --declaration",
    "preprod": "tsc && node -r dotenv/config ./dist/index.js",
    "start": "node dist/index.js",
    "startdev": "tsc && node -r dotenv/config dist/index.js"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./src/test/setup.ts"
    ]
  },
  "keywords": [
    "MSSQL",
    "Import",
    "Database",
    "CSV",
    "Excel",
    "Copy&Paste"
  ],
  "author": "Michael Ronge",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/express-ntlm": "^2.3.3",
    "@types/jest": "^29.5.1",
    "@types/mssql": "^9.1.0",
    "@types/node": "^22.9.0",
    "@types/supertest": "^6.0.0",
    "@types/tsqlstring": "^1.0.0",
    "dotenv": "^16.0.3",
    "grunt": "^1.6.1",
    "grunt-bump": "^0.8.0",
    "jest": "^29.5.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "express": "^4.18.2",
    "express-ntlm": "^2.6.2",
    "express-validator": "^7.0.1",
    "mssql": "^11.0.0",
    "tsqlstring": "^1.0.1"
  }
}
